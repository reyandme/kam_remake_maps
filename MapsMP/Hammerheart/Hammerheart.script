{$I difficulty.script}
{$I improvestaticai.script}


{$COMMAND cmd: ShowCommands}
{$COMMAND skipopeningfight: SkipInitialFightSequence}
{$COMMAND killmygroups: KillPlayerGroups}


var
	bGoldMsg: array[0..16] of Boolean;



procedure ShowCommands(P: Integer);
begin
	Actions.ShowMsg(P, '/cmd|/skipopeningfight|/killmygroups');
end;


procedure KillPlayerGroups(P: Integer);
var
	g: ShortInt;
	iGroups: array of Integer;
begin
	iGroups := States.PlayerGetAllGroups(P);
	for g := 0 to Length(iGroups) -1 do
		Actions.GroupKillAll(iGroups[g], false);
end;


procedure SkipInitialFightSequence(P: Integer); //Kill all Groups involved in initial Attack
var
	g: Byte;
begin
	for g := 13 to 15 do
		KillPlayerGroups(g);
end;


function MinToTick(Minutes: Word): Word;
begin
	Result := Minutes *600;
end;


function Randomize(Value: Integer; Percentage: Single): Integer;
begin
	Result := Utils.RandomRangeI(Round(Value *(1 -(Percentage /100))), Round(Value *(1 +(Percentage /100))));
end;


procedure IronEquipRate(Player: Byte; Rate: Word);
begin
	Actions.AIEquipRate(Player, 1, Rate);
	Actions.AIEquipRate(Player, 0, Rate *2); //Leather Equip Rate = Iron Equip Rate *2
end;


