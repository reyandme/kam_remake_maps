procedure OnHouseBuilt(aHouse: Integer);
var
	HouseOwner: Byte;
begin
	//Unlock Metallurgist's after Sawmill
	if States.HouseType(aHouse) = 0 then
	begin
		HouseOwner := States.HouseOwner(aHouse);
		if not bGoldMsg[HouseOwner] then	
		begin
			Actions.HouseUnlock(HouseOwner, 15);
			Actions.ShowMsg(HouseOwner, '<$1>'); //Message to Player
			bGoldMsg[HouseOwner] := true;
		end;
	end;
end;


procedure OnTick();
var
	GameTime: Cardinal;
begin
	GameTime := States.GameTime();
	
	//Initial Message
	if GameTime = 30 then
		Actions.ShowMsg(-1, '<$0>');
end;