function IsMineableResource(TileType: Integer): Boolean;
begin
	case TileType of
		263, 155, 154, 153, 152:		Result := true; //Coal
		144, 145, 146, 147, 307: 	  	Result := true; //Gold
		148, 149, 150, 151, 259, 260: 	Result := true; //Iron
		else
			Result := false;
	end;
end;


function GetRandomDirtTile(): Integer;
begin
	case Utils.RandomRangeI(0, 8) of
		0..4: 	Result := 35; //Normal Dirt-Tile
		5..7: 	Result := 36;
		8:		Result := 37; //Stone-Dirt-Tile
	end;
end;


function GetRandomEmptyGoldTile(): Integer;
begin
	case Utils.RandomRangeI(0, 8) of
		0,1: Result := 159;
		2,3: Result := 158;
		4,5: Result := 157;
		6,7: Result := 201;
		8:	 Result := 156; //Don't like this tile much lol
	end;
end;


function GetSmallerResource(TileType, Turns: Integer): Integer;
var
	t: Integer;
begin
	while t < Turns do
	begin
		case TileType of
			//Coal
			263: TileType := 155;
			155: TileType := 154;
			154: TileType := 153;
			153: TileType := 152;
			152: TileType := GetRandomDirtTile(); //Dirt Tile
			//Gold
			307: TileType := 147;
			147: TileType := 146;
			146: TileType := 145;
			145: TileType := 144;
			144: TileType := GetRandomEmptyGoldTile(); //Empty Gold Mountain Tile
			//Iron
			260: TileType := 259;
			259: TileType := 151;
			151: TileType := 150;
			150: TileType := 149;
			149: TileType := 148;
			148: TileType := 160 +Utils.RandomRangeI(0, 3); //Empty Iron Tile
		end;
		Inc(t);
	end;
	Result := TileType;
end;



procedure ReduceResourceAmount(X1, Y1, X2, Y2, ReduceBy: Integer);
var
	X, Y, MapTileType: Integer;
begin
	for X := X1 to X2 do
		for Y := Y1 to Y2 do
		begin
			MapTileType := States.MapTileType(X, Y);
			if IsMineableResource(MapTileType) then
				Actions.MapTileSet(X, Y, GetSmallerResource(MapTileType, ReduceBy), Utils.RandomRangeI(0, 3));
		end;
end;


