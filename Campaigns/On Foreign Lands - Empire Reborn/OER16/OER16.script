procedure OnTick;
begin
	SU_ExecuteKill();
	if ((variant = false) AND (Length(States.PlayerGetAllUnits(1)) <= 0) AND (Length(States.PlayerGetAllUnits(2)) <= 0) AND (Length(States.PlayerGetAllHouses(1)) <= 0) AND (Length(States.PlayerGetAllHouses(2)) <= 0)) then Actions.PlayerWin([0], false);
	
	if (variant = true) then begin
		if (States.GameTime = 50) then Actions.ShowMsg(0, '<$1>');
		if (States.GameTime = 350) then Actions.ShowMsg(0, '<$2>');
		if (States.GameTime = 500) then Actions.OverlayTextSet(0, '<$3>');
		if (States.GroupAt(8, 127) > 0) then begin
			Actions.PlayerWin([0], true);
			Actions.GroupKillAll(States.GroupAt(8, 127), true);
		end;
	end
	else begin
		if ((States.StatArmyCount(0) < 5) AND (supportArrived = false)) then begin
			Actions.GiveGroup(0,22,4,26,5,12,4);
			Actions.GiveGroup(0,18,16,23,3,12,4);
			SU_RegisterSuperGroup(States.GroupAt(4, 26));
			SU_RegisterSuperGroup(States.GroupAt(16, 23));
			Actions.GroupHungerSet(States.GroupAt(4, 26), States.UnitMaxHunger);
			Actions.GroupHungerSet(States.GroupAt(16, 23), States.UnitMaxHunger);
			Actions.ShowMsgGoto(0, 4, 26, '<$4>');
			supportArrived := true;;
		end;
	end;
	
	if (((Length(States.PlayerGetAllHouses(1)) + Length(States.PlayerGetAllHouses(2))) < 4) AND (textShown = false) AND (variant = false)) then begin
		Actions.OverlayTextSet(0, '<$3>');
		textShown := true;
		victoryTime := States.GameTime + 300;
	end;
	
	if ((textShown = true) AND (States.GameTime >= victoryTime)) then Actions.PlayerWin([0], false);
	
	if ((group > 0) AND (States.GroupDead(group)) AND (liteArrived = false) AND (liteAlive = true) AND (variant = false)) then begin
		liteArrived := true;
		Actions.GiveGroup(7, 15, 24, 7, 5, 12, 4);
		Actions.GiveGroup(7, 17, 10, 4, 4, 12, 4);
		villageArriveTime := States.GameTime + 300;
		Actions.ShowMsg(0, 'Lite przybylo!');
	end;
	
	if ((group > 0) AND (States.GroupDead(group)) AND (States.GameTime = villageArriveTime) AND (villageChosen <> 2) AND (allyArrived = false) AND (variant = false)) then begin
		allyArrived := true;
		if (villageChosen = 1) then begin
			Actions.GiveGroup(6, 23, 24, 7, 5, 12, 4);
			Actions.GiveGroup(6, 17, 10, 4, 4, 12, 4);
			Actions.GroupHungerSet(States.GroupAt(24, 7), States.UnitMaxHunger);
			Actions.GroupHungerSet(States.GroupAt(10, 4), States.UnitMaxHunger);
			Actions.ShowMsg(0, 'Foll przybylo!');
		end
		else if (villageChosen = 3) then begin
			Actions.GiveGroup(5, 24, 24, 7, 5, 12, 4);
			Actions.GiveGroup(5, 25, 10, 4, 4, 12, 4);
			Actions.GroupHungerSet(States.GroupAt(24, 7), States.UnitMaxHunger);
			Actions.GroupHungerSet(States.GroupAt(10, 4), States.UnitMaxHunger);
			Actions.ShowMsg(0, 'Loren przybylo!');
		end;
	end;
end;

procedure OnUnitDied(aUnit: Integer; aAttacker: Integer);
begin
	SU_ExecuteSuperunitDie(aUnit, aAttacker);
	if ((States.StatArmyCount(0) <= 0) AND (States.GameTime > 100)) then Actions.PlayerDefeat(0);
end;

procedure OnUnitAttacked(aUnit: Integer; aAttacker: Integer);
begin
	SU_ExecuteUnitAttack(aUnit, aAttacker);
end; 
 
